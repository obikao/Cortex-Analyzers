  "baseConfig": "DomainTools",
  "config": {
      "service": "risk_evidence"
  },
  "configurationItems": [
    {
      "name": "username",
      "description": "DomainTools API credentials",
      "type": "string",
      "multi": false,
      "required": true
    },
    {
      "name": "key",
      "description": "DomainTools API credentials",
      "type": "string",
      "multi": false,
      "required": true
    }
  ]
}
